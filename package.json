{
  "name": "launchdz",
  "description": "Guided creation of launchd plist files",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/jackmac92/launchd-wizard"
  },
  "version": "v2.6.0",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "launchdz"
        }
      ]
    ]
  },
  "scripts": {
    "build": "pika build",
    "check-commit": "lint-staged && jest --changedSince=HEAD",
    "test": "jest",
    "lint": "eslint src/**/*.js"
  },
  "dependencies": {
    "@sentry/node": "^5.15.4",
    "enquirer": "^2.3.2",
    "plist": "^3.0.1",
    "shelljs": "^0.8.3",
    "typescript": "^3.7.4",
    "write": "^2.0.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.2.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-simple-bin": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.8.1",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.13.4",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "commitlint": "^8.2.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "pre-commit": "^1.2.2",
    "ts-jest": "^24.2.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
