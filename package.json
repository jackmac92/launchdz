{
  "name": "launchdz",
  "description": "Guided creation of launchd plist files",
  "version": "1.0.1",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "cli-name"
        }
      ]
    ]
  },
  "scripts": {
    "build": "pika build",
    "check-commit": "lint-staged && jest --changedSince=HEAD",
    "test": "jest",
    "lint": "eslint src/**/*.js"
  },
  "dependencies": {
    "enquirer": "^2.3.2",
    "plist": "^3.0.1",
    "shelljs": "^0.8.3",
    "write": "^2.0.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.1.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.6.1",
    "@pika/plugin-simple-bin": "^0.6.1",
    "@pika/plugin-standard-pkg": "^0.6.1",
    "commitlint": "^8.1.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "pre-commit": "^1.2.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
